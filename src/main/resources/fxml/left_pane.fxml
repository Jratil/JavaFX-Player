<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootPane" maxWidth="250" minWidth="250" prefHeight="630.0" prefWidth="250.0" style="-fx-background-color: aquamarine"
            stylesheets="/css/music_pane.css" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jratil.controller.LeftController">
    <children>
        <ScrollPane fx:id="scrollPane" maxHeight="630.0" minHeight="630.0">
            <content>
                <VBox prefWidth="250.0">
                    <children>
                        <VBox id="vbox" prefHeight="199.0" prefWidth="100.0">
                            <children>
                                <Label text="MV" textFill="#6b6b6b">
                                    <VBox.margin>
                                        <Insets left="10.0" top="10.0"/>
                                    </VBox.margin>
                                    <font>
                                        <Font size="16.0"/>
                                    </font>
                                </Label>
                                <ListView fx:id="mvListView" prefHeight="200.0" prefWidth="200.0">
                                    <VBox.margin>
                                        <Insets top="10.0"/>
                                    </VBox.margin>
                                </ListView>
                            </children>
                        </VBox>
                        <VBox id="vbox"  prefHeight="199.0" prefWidth="100.0">
                            <children>
                                <Label text="我的音乐" textFill="#6b6b6b">
                                    <VBox.margin>
                                        <Insets left="10.0" top="10.0"/>
                                    </VBox.margin>
                                    <font>
                                        <Font size="16.0"/>
                                    </font>
                                </Label>
                                <ListView fx:id="myMusicListView" prefHeight="200.0" prefWidth="200.0">
                                    <VBox.margin>
                                        <Insets top="10.0"/>
                                    </VBox.margin>
                                </ListView>
                            </children>
                        </VBox>
                        <Accordion prefHeight="200.0">
                            <panes>
                                <TitledPane fx:id="createList" text="创建的歌单" textFill="#727272">
                                    <content>
                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
                                                    prefWidth="200.0"/>
                                    </content>
                                </TitledPane>
                                <TitledPane fx:id="collectionList" text="收藏的歌单" textFill="#6b6b6b">
                                    <content>
                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0"
                                                    prefWidth="200.0"/>
                                    </content>
                                </TitledPane>
                            </panes>
                        </Accordion>
                    </children>
                </VBox>
            </content>
        </ScrollPane>
    </children>
</AnchorPane>
